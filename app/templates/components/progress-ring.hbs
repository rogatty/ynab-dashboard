<svg
  height="{{doubleRadius}}"
  width="{{doubleRadius}}"
>
  <circle
    stroke="black"
    stroke-dasharray="{{circumference}} {{circumference}}"
    style="stroke-dashoffset:{{offset}}"
    stroke-width="{{stroke}}"
    fill="transparent"
    r="{{normalizedRadius}}"
    cx="{{radius}}"
    cy="{{radius}}"
  />
  <text x="50%" y="50%" text-anchor="middle">{{progress}}%</text>
</svg>
